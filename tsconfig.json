{
	"compilerOptions": {
		"target": "ES5",
		"moduleDetection": "auto",
		"module": "CommonJS",
		"moduleResolution": "node",
		"inlineSourceMap": true,
		"inlineSources": true,
		"outDir": "./bin",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*"]
}
